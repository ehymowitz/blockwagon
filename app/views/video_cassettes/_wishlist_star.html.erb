<% if @video_cassette.user != current_user %>
  <% if @user_wishlist %>
    <%= link_to wishlist_path(@user_wishlist), method: 'delete' do%>
      <i class="wish fas fa-star" class="pb-3"></i>
    <% end %>
  <% else %>
    <%= link_to video_cassette_wishlists_path(@video_cassette), method: 'post' do%>
      <i class="wish far fa-star mb-3"></i>
    <% end %>
  <% end %>
<% end %>